  "devDependencies": {
    "@types/aes-js": "^3.1.1",
    "webpack-cli": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add"
    ],
    "*.yml": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist",
  "private": true,
  "repository": "",
  "scripts": {
    "b": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "build": "rm -rf dist/* && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" --ignore *.spec.js --out-dir dist --copy-files --source-maps --verbose",
    "dev": "cross-env NODE_OPTIONS=\"--trace-warnings\" NODE_ENV=development ts-node ./src/index.ts",
    "lint": "eslint --ext js --ext ts 'src/**'",
    "lint:staged": "lint-staged",
    "prestart": "npm run -s build",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "start": "ts-node dist",
    "test": "eslint src",
    "typecheck": "tsc --pretty",
    "webpack": "webpack --watch --progress --config webpack.config.js",
    "w": "yarn b webpackx.ts"
  }
}
